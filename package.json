{
  "name": "d0ma1n",
  "version": "0.1.0",
  "description": "Domain & brand name spoofing scanner powered by SSIM-scored confusable maps",
  "type": "module",
  "bin": {
    "d0ma1n": "dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
