    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "namespace-guard": "^0.19.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0",
    "wrangler": "^4.69.0"
  },
  "keywords": [
    "domain",
    "spoofing",
    "homoglyph",
    "confusable",
    "phishing",
    "brand-protection",
    "unicode",
    "idn",
    "punycode",
    "dns"
  ],
  "license": "MIT",
  "author": "Paul Wood (@paultendo)",
  "repository": {
    "type": "git",
    "url": "https://github.com/paultendo/d0ma1n"
  }
